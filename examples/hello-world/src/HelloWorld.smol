union Nat {
    Zero
    Succ Nat
} numeric
union Bool { True, False }

sub = \a b {
    a :: Nat
    b :: Nat
    b <= a
    returns :: Nat

    sub a 0 = a
    sub (Nat.Succ a) (Nat.Succ b) = sub a b
}
